        <string name="Source"><![CDATA[local ReplicatedStorage = game:GetService("ReplicatedStorage")

local tool = script.Parent
local assetId = tool.Name

local placementModule = ReplicatedStorage:WaitForChild("Placement")
local controllerModule = placementModule:WaitForChild("Client"):WaitForChild("Controller")
local Controller = require(controllerModule)

local function startPlacement()
	Controller:Start()
	local token = tool:GetAttribute("PlacementToken")
	local level = tool:GetAttribute("PlacementLevel")
	local ok, err = pcall(function()
		Controller:StartPlacement(assetId, token, level)
	end)
	if not ok then
		warn(`Failed to start placement for "{assetId}": {err}`)
	end
end

local function stopPlacement()
	if Controller.GetActiveAsset and Controller:GetActiveAsset() == assetId then
		Controller:StopPlacement()
	end
end

tool.Equipped:Connect(startPlacement)
tool.Unequipped:Connect(stopPlacement)
tool.AncestryChanged:Connect(function(_, parent)
	if not parent then
		stopPlacement()
	end
end)

tool.Destroying:Connect(function()
	stopPlacement()
end)
]]></string>
