        <string name="Source"><![CDATA[local ReplicatedStorage = game:GetService("ReplicatedStorage")

local tool = script.Parent

local placementModule = ReplicatedStorage:WaitForChild("Placement") :: ModuleScript
local clientFolder = placementModule:WaitForChild("Client")
local inspectorModule = clientFolder:WaitForChild("UpgradeInspector") :: ModuleScript
local UpgradeInspector = require(inspectorModule)

local inspector = UpgradeInspector.new(tool)

local function unequip()
	inspector:Disable()
end

tool.Equipped:Connect(function()
	inspector:Enable()
end)

tool.Unequipped:Connect(unequip)

tool.Activated:Connect(function()
	inspector:HandleActivation()
end)

tool.AncestryChanged:Connect(function(_, parent)
	if parent == nil then
		unequip()
	end
end)

tool.Destroying:Connect(function()
	inspector:Destroy()
end)
]]></string>
